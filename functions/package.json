    "email-validator": "^2.0.4",
    "expo": "~50.0.17",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^6.1.0",
    "password-validator": "^5.3.0",
    "phone": "^3.1.51",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "util": "^0.12.5",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
